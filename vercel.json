{
  "version": 2,
  "functions": {
    "api/vercel-ssr/index.js": {
      "includeFiles": "dist/**"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/api/vercel-ssr/index.js"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}